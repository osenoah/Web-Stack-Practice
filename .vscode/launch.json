{
    "configurations": [
        
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "${file}",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "url": "${file}",
            "webRoot": "${workspaceFolder}"
        },
            {
                "name": "Launch Chrome",
                "request": "launch",
                "type": "chrome",
                "timeout": 20000,
                "url": "${file}",
                "webRoot": "${workspaceFolder}"
        },
    ]
}